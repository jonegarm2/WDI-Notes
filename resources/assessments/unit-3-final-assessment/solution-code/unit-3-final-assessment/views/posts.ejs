<!DOCTYPE html>
<html>
  <head>
    <title>Unit 3 Assessment</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div>
      <a href="/">HOME</a> | <a href="/posts">ALL POSTS</a>
    </div>
    <h1>All Posts</h1>
    <table>
      <tr><th>Title</th><th>Body</th></tr>
      <% posts.forEach(function(post) { %>
        <tr>
          <td><%= post.title %></td>
          <td><%= post.body %></td>
        </tr>
      <% }); %>
    </table>
  </body>
</html>
