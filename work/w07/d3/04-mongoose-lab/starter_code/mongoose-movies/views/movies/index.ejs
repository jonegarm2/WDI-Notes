<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>All Movies</title>
</head>
<body>
  <h2>All Movies</h2>
  <table>
    <tr>
      <th>Title</th>
      <th>Year<br>Released</th>
      <th>Rating</th>
      <th>Cast</th>
      <th>Now<br>Showing</th>
    </tr>
    <% movies.forEach(function(m) { %>
      <tr>
        <td><%= m.title %></td>
        <td><%= m.releaseYear %></td>
        <td><%= m.rating %></td>
        <td><%= m.cast.join(', ') %></td>
        <td><%= m.nowShowing ? 'Yes' : 'No' %></td>
      </tr>
    <% }); %>
  </table>
</body>
</html>